{
  "name": "color-pallet-generator",
  "version": "0.1.0",
  "description": "A tool to automagically generate color pallets for a project.",
  "scripts": {
    "preview": "http-server ./docs/",
    "compile": "npm-run-all compile:*",
    "compile:typescript": "tsc",
    "compile:sass": "node ./sass.config.js",
    "build": "npm run compile && npm run preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codewithkyle/color-pallet-generator.git"
  },
  "author": "Kyle Andrews",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codewithkyle/color-pallet-generator/issues"
  },
  "homepage": "https://github.com/codewithkyle/color-pallet-generator#readme",
  "dependencies": {
    "node-sass": "^4.12.0",
    "rollup": "^1.19.4",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "glob": "^7.1.4",
    "http-server": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
